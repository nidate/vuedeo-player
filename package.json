{
  "name": "vuedeo-player",
  "author": "nidate@core-note.jp",
  "description": "Video player application written in electron and vue",
  "version": "0.2.2",
  "private": true,
  "license": "GPL-3.0-or-later",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nidate/vuedeo-player.git"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "serve": "vite preview",
