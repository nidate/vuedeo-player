  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "video.js": "^8.10.0",
    "videojs-abloop": "^1.2.0",
    "videojs-hotkeys": "^0.2.28",
    "vite": "^5.0.12",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.15"
